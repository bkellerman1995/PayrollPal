                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT 
    PE.IdEncabezado, 
    PE.Codigo, 
    PE.TipoCambio, 
    PE.TotalIngresos, 
    PE.TotalGastos, 
    PE.TotalPagar, 
    PD.IdDetalle, 
    PD.IdColaborador, 
    PD.NombreColaborador, 
    PD.hrsTrabajadas, 
    PD.hrsExtras, 
    PD.MontoHora, 
    PD.SalarioBruto, 
    PD.SalarioNeto, 
    PP.FechaPago, 
    PP.FechaDesde, 
    PP.FechaHasta, 
    C.SalarioHora,

(
    SELECT 
        STUFF((
            SELECT ', ' + 
                   CASE 
                      WHEN dp.Tipo = 1 THEN dp.Nombre + ' (' + CAST(dp.Valor AS VARCHAR(20)) + '%)'
                      ELSE dp.Nombre 
                   END + 
                   CASE 
                      WHEN dp.Tipo = 1 THEN ' - ₡' + 
                          CAST(CASE WHEN dp.TipoValor = 0 THEN PD.SalarioBruto * (dp.Valor / 100)
                                    WHEN dp.TipoValor = 1 THEN dp.Valor 
                               END AS VARCHAR(20)) 
                      ELSE '' 
                   END
            FROM Deducciones_Percepciones_Por_Colaborador AS dpp 
            INNER JOIN Deducciones_Percepciones AS dp ON dpp.CodigoDeduccionPercepcion = dp.CodigoDeduccionPercepcion
            WHERE dpp.Estado = 1 
                  AND PD.IdColaborador = C.IdColaborador
                  AND dpp.IdColaborador = PD.IdColaborador
                  AND dp.Tipo = 1 -- Filter by deduction type
            FOR XML PATH('')), 1, 2, '')
) AS DeduccionesList,

	 (
        SELECT 
            STUFF((
                SELECT ', ' + 
                       CASE 
                          WHEN dp.Tipo = 0 THEN dp.Nombre
                          ELSE dp.Nombre + ' (' + CAST(dp.Valor AS VARCHAR(20)) + '%)'
                       END + 
                       CASE 
                          WHEN dp.Tipo = 0 THEN ''
                          ELSE ' + ₡' + 
                              CAST(CASE WHEN dp.TipoValor = 0 THEN PD.SalarioBruto * (dp.Valor / 100)
                                        WHEN dp.TipoValor = 1 THEN dp.Valor 
                                   END AS VARCHAR(20)) 
                       END
                FROM Deducciones_Percepciones_Por_Colaborador AS dpp 
                INNER JOIN Deducciones_Percepciones AS dp ON dpp.CodigoDeduccionPercepcion = dp.CodigoDeduccionPercepcion
                WHERE dpp.Estado = 1 
                      AND PD.IdColaborador = C.IdColaborador
                      AND dpp.IdColaborador = PD.IdColaborador
                      AND dp.Tipo = 0 -- Filter by perception type
                FOR XML PATH('')), 1, 2, '')
    ) AS PercepcionesList,

(SELECT 
     STUFF((SELECT 
                 ', ' + ' - Desde ' + FORMAT(SV.FechaSolicitarDesde, 'dd/MM/yyyy') + ' Hasta ' + FORMAT(SV.FechaSolicitarHasta, 'dd/MM/yyyy') + ' (Días: ' + CAST(SV.CantidadDias AS VARCHAR(10)) + ')' 
             FROM 
                 SolicitudVacaciones AS SV 
             WHERE 
                 SV.IDColaborador = PD.IdColaborador 
                 AND SV.Estado = 1 
                 AND SV.Observaciones_Final = 0
             FOR XML PATH('')), 1, 2, '') AS ListaVacaciones
) AS Vacaciones
FROM 
    Planilla_Encabezado AS PE 
INNER JOIN 
    Planilla_Detalle AS PD ON PE.IdEncabezado = PD.IdEncabezado 
INNER JOIN 
    PlanillaPago AS PP ON PE.Codigo = PP.Codigo 
INNER JOIN 
    Colaborador AS C ON PD.IdColaborador = C.IdColaborador
WHERE 
    PE.IdEncabezado = @IdEncabezado;

</CommandText>
